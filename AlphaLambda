/* Author: Luigi Vincent
My first ever use of a Lambda expression. Simply counting 1 -5
*/

import java.util.Arrays;
import java.util.List;

public class LambdaEx {
	public static void main(String[] args) {
		List<Integer> ints = Arrays.asList(1, 2, 3, 4, 5);

		ints.forEach(x -> System.out.println(x));
		/* forEach is a method that calls the function passed for each value on the list
		x -> ... is a lambda expression with parameter x of type Integer */
	}
}
